<?xml version='1.0' encoding='utf-8'?>
<widget id="com.beautonomous.first" version="0.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FirebasePlugin">
        <param name="android-package" value="org.apache.cordova.firebase.FirebasePlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Be Autonomous : Window of Life</name>
    <description>
      Track your life easily.
    </description>
    <author email="thisismyidashish@gmail.com" href="http://beautonomous.herokuapp.com/">
      Ashish Negi
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/logo.png" />
    <icon density="mdpi" src="res/icon/android/logo.png" />
    <icon density="hdpi" src="res/icon/android/logo.png" />
    <icon density="xhdpi" src="res/icon/android/logo.png" />
    <icon density="xxhdpi" src="res/icon/android/logo.png" />
    <icon density="xxxhdpi" src="res/icon/android/logo.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
